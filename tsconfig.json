{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@repositories/*": ["repositories/*"],
      "@components/*": ["src/components/*"],
      "@actions/*": ["src/actions/*"]
    }
  }
}
