---
export const prerender = false;
import ProductPage from "@/modules/product/pages/products/product-page.astro";

const { id } = Astro.params;

Astro.response.headers.set(
  "Cache-Control",
  "public, max-age=0, s-maxage=31536000, stale-while-revalidate=0"
);

Astro.response.headers.set("netlify-cache-tag", `product/${id}`);
---

<ProductPage id={id!} />
